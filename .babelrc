{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "modules": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-external-helpers",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": { "version": 3, "proposals": true },
        "useESModules": true
      }
    ]
  ]
}
